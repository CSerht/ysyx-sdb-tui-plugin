{
  "name": "ysyx-sdb-tui",
  "displayName": "ysyx-sdb-tui",
  "description": "The SDB TUI for ysyx NEMU or NPC",
  "icon": "image/icon.jpg",
  "version": "0.0.2",
  "publisher": "CSerHtDuck",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.ysyx-sdb-tui-Enable",
        "title": "ysyx sdb tui Enable"
      },
      {
        "command": "extension.ysyx-sdb-tui-Disable",
        "title": "ysyx sdb tui Disable"
      }
    ],
    "properties": {
      "ysyxSdbTUI.serverPort": {
        "type": "number",
        "default": 49159,
        "description": "Socket server port"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.2"
  },
   "repository": {
    "type": "git",
    "url": "https://github.com/CSerht/ysyx-sdb-tui-plugin.git"
  }
}
